@article{schnoerrApproximationInferenceMethods2017,
  title = {Approximation and Inference Methods for Stochastic Biochemical Kinetics - a Tutorial Review},
  author = {Schnoerr, David and Sanguinetti, Guido and Grima, Ramon},
  date = {2017-03-03},
  journaltitle = {Journal of Physics A: Mathematical and Theoretical},
  shortjournal = {J. Phys. A: Math. Theor.},
